          data[\"hemoglobin\"] = data[\"hemoglobin\"].split(\" \")[0];\n            data[\"bloodPressure\"] = data[\"bloodPressure\"].split(\" \")[0];\n            data[\"bloodSugar\"] = data[\"bloodSugar\"].split(\" \")[0];\n\n            setCurrent(data);\n            setEdit(true);\n            setPhotoUrl(doc.data().candidatePhoto);\n            setFetching(false);\n          } else {\n            alert(\"no such report found\");\n            console.log(\"no such doc\");\n            history.push(\"/dashboard/reports\");\n          }\n        } catch (err) {\n          console.log(err);\n        }\n      }\n    }\n\n    fetchData();\n  }, []);\n\n  return (\n    <>\n      {fetching && (\n        <Container className=\"pt-4 text-center\">\n          <img src=\"/assets/images/loader.gif\" alt=\"loader\" />\n        </Container>\n      )}\n      {!fetching && (\n        <Container className=\"pt-4\">\n          <Row className=\"fill-report-icon text-center\">\n            <Col>\n              <img\n                src=\"/assets/images/fill-report.png\"\n                alt=\"fill-report-icon\"\n                style={{ width: \"5rem\" }}\n              />\n            </Col>\n          </Row>\n          <Form onSubmit={(e) => {e.preventDefault()}}>\n            <br />\n            <br />\n            <Row>\n              <Col>\n                <Card>\n                  <Card.Body>\n                    <Card.Title>Lab Sr No.</Card.Title>\n                    <Form.Group>\n                      <Form.Control\n                        type=\"text\"\n                        ref={labSrNoRef}\n                        disabled={true}\n                        value={\n                          edit ? current.labSrNo : `MT_${current.lab + 1}`\n                        }\n                      />\n                    </Form.Group>\n                  </Card.Body>\n                </Card>\n              </Col>\n              <Col>\n                <Card>\n                  <Car